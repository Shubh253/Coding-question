for(int i = 0;i<n-1;i++){
   int min = i;
    for(int j = i+1;j<n;j++){
       if(a[j]<a[min]){
          min = j;
          }
       }
    if(min!=i){
      int temp = a[i];
      a[i] = a[min];
      a[min] = temp;
      }
     }
for(int item:a){
   System.out.print(items+" ")
   }
          
       
